<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="PrismAppExample.Views.ViewPdfView">

    <StackLayout>

        <Button Text="Open Embedded PDF" Command="{Binding OpenPdfCommand}"/>
        <Button Text="Online PDF using Pdf.js in WebView" Command="{Binding OpenGoogleDocsPdfCommand}"/>


    </StackLayout>
    
</ContentPage>